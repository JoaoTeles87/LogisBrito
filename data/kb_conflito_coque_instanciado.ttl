@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rec: <http://recife.leg.br/ontologia-conflito#> .

rec:Arrecadacao_Perdida a owl:Class ;
    rdfs:label "Arrecadação Perdida" ;
    rdfs:subClassOf rec:DanoUrbano .

rec:Caos_Funcional a owl:Class ;
    rdfs:label "Caos Funcional" ;
    rdfs:subClassOf rec:DanoUrbano .

rec:Centro_Ocioso a owl:Class ;
    rdfs:label "Centro Ocioso" ;
    rdfs:subClassOf rec:EspacoDeConflito .

rec:Doenca_e_Morte a owl:Class ;
    rdfs:label "Doença e Morte" ;
    rdfs:subClassOf rec:DanoUrbano .

rec:Lei_do_Remembramento_2020 a owl:NamedIndividual ;
    rec:causa_direta rec:Risco_de_Gentrificacao_Coque .

rec:Mercado_Imobiliario_Especulativo a rec:Elite_Proprietaria ;
    rec:em_antagonismo_com rec:Comunidade_do_Coque .

rec:OODC a owl:Class ;
    rdfs:label "OODC - Outorga Onerosa do Direito de Construir" ;
    rdfs:subClassOf rec:InstrumentoAcao .

rec:PEUC a owl:Class ;
    rdfs:label "PEUC - Parcelamento, Edificação ou Utilização Compulsórios" ;
    rdfs:subClassOf rec:InstrumentoAcao .

rec:Prefeitura_do_Recife a rec:Estado_Inativo ;
    rec:impede rec:Lei_do_PREZEIS_1995 .

rec:ZEIS_Coque a rec:ZEIS .

rec:causa_direta a owl:ObjectProperty ;
    rdfs:comment "Relação causal: X causa diretamente Y. (Ex: Inação causa Dano)" .

rec:em_antagonismo_com a owl:SymmetricProperty ;
    rdfs:comment "Relação de disputa: X e Y têm objetivos conflitantes." .

rec:financia_a_reversao a owl:ObjectProperty ;
    rdfs:comment "Relação instrumental: X gera recursos para financiar a solução Y." .

rec:impede a owl:ObjectProperty ;
    rdfs:comment "Relação de bloqueio: X impede a ocorrência/aplicação de Y. (Ex: Veto impede Ação)" .

rec:metaQuantificavel a owl:DatatypeProperty ;
    rdfs:comment "Define um contrato de desempenho numérico para um instrumento." .

rec:nivelDeAcao a owl:DatatypeProperty ;
    rdfs:comment "Classifica a 'audácia' de um instrumento: Passivo, Punitivo_Moderado, Punitivo_Agressivo." .

rec:reverte_por_forca a owl:ObjectProperty ;
    rdfs:comment "Relação de poder: X reverte o efeito de Y. (Ex: Instrumento reverte Veto)" .

rec:Comunidade_do_Coque a owl:NamedIndividual .

rec:Elite_Proprietaria a owl:Class ;
    rdfs:label "Elite Proprietária" ;
    rdfs:subClassOf rec:VetoInacao .

rec:Estado_Inativo a owl:Class ;
    rdfs:label "Estado Inativo" ;
    rdfs:subClassOf rec:VetoInacao .

rec:Lei_do_PREZEIS_1995 a rec:InstrumentoAcao ;
    rec:reverte_por_forca rec:Risco_de_Gentrificacao_Coque .

rec:ZEIS a owl:Class ;
    rdfs:label "ZEIS - Zona Especial de Interesse Social" ;
    rdfs:subClassOf rec:EspacoDeConflito .

rec:EspacoDeConflito a owl:Class ;
    rdfs:label "Espaço de Conflito" .

rec:Risco_de_Gentrificacao_Coque a rec:DanoUrbano .

rec:VetoInacao a owl:Class ;
    rdfs:label "Veto & Inaction" ;
    rdfs:comment "A classe de todas as ações ou omissões que impedem o desenvolvimento urbano justo." .

rec:InstrumentoAcao a owl:Class ;
    rdfs:label "Instrumento & Ação" .

rec:DanoUrbano a owl:Class ;
    rdfs:label "Dano & Consequência" .

